{
  "name": "anonance-api",
  "version": "1.0.0",
  "description": "This a is backend service for Annonance developed",
  "author": "Oluwkayode Fafiyebi <oluwakayodefafiyebi95@gmail.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node build/server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.6",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/proxy-addr": "^2.0.3",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^9.0.7",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.1",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.1",
    "typescript": "~4.6",
    "youch": "^3.3.3",
    "youch-terminal": "^2.2.3"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.2",
    "@adonisjs/repl": "^3.1.11",
    "@binance/connector-typescript": "^0.3.4",
    "@slack/bolt": "^3.17.0",
    "@types/jsonwebtoken": "^9.0.5",
    "adonis5-bullmq": "^3.2.0",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "mailtrap": "^3.2.0",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "npm": "^10.2.5",
    "phc-argon2": "^1.1.4",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.14",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  }
}
